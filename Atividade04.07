Questão 3:
//Declaração de variáveis
    real kilos, valor, nota, troco, falta

    //Entrada de dados
    escreva("Informe o peso do seu prato em kg: ")
    leia(kilos)
    escreva("Informe o valor da sua nota: ")
    leia(nota)

    //Processamento
    valor = kilos * 12.0 
    troco = nota - valor
    falta = valor - nota
    escreva("O valor a pagar será: ", valor, " reais", "\n")
    se(nota < valor) 
      escreva("Valor insuficiente, falta: ", falta, "\n")
    senao
      escreva("Seu troco será de: ", troco, "\n")

Questão 4:
//Declaração de variáveis 
inteiro dia, mes, dias_passados 

//Entrada de dados 
escreva("Informe o dia: ") 
leia(dia) 
escreva("Informe o mês: ") 
leia(mes)

//Processamento 
dias_passados = (mes * 30) + dia 

//Saída de dados 
escreva("Desde o começo do ano se passaram: ", dias_passados, " dias.")

